{% extends "index.njk" %}

{% block content %}
    <h1 class="page-title">Users page</h1>
    <a class="btn btn-success" href="users/create">Create</a>
    <ul>
        {% for user in users %}
            <li><a href="users/{{ user.id }}">{{ user.first_name }} {{ user.last_name }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}
